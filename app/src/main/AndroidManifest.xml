<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sd.swipemenu">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.sd.swipemenu.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.sd.swipemenu.SimpleActivity" />
        <activity android:name="com.sd.swipemenu.ListViewActivity" />
        <activity android:name="com.sd.swipemenu.NestedActivity" />
        <activity android:name="com.sd.swipemenu.InfiniteSwipeActivity" />

    </application>

</manifest>